  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b9b"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#605b"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#f00"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#aa6666"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#ea9292"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#00d4eb"
      }
    },
    {
      "name": "Storage", // const, let, =
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.control.import",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#e69d00"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#ff6644"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#069b29"
      }
    },
    {
      "name": "HTML textContent and meta",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#5d8d33"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#f5800b"
      }
    },
    {
      "name": "XML tag fluff",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#ffaa11"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#0077BB"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#0f7dfa"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block"],
      "settings": {
        "foreground": "#e16067"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.object.property",
        "variable.other.property",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#7600bb"
      }
