{
  "name": "Boundless moa",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#1e1e1e",
    "activityBar.background": "#131313",
    "tab.inactiveBackground": "#131313",
    "editorGroupHeader.tabsBackground": "#131313",
    "tab.border": "#1a1a1a",
    "activityBar.activeBackground": "#1a1a1a",
    "sideBar.background": "#1a1a1a",
    "panel.background": "#1a1a1a",
    "sideBarSectionHeader.background": "#1a1a1a",
    "statusBar.background": "#1e1e1e",
    "statusBar.noFolderBackground": "#1e1e1e",
    "tab.activeBackground": "#1e1e1e",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb",
    "diffEditor.insertedTextBackground": "#2c440088",
    "sash.hoverBorder": "#007acc",
    "editor.selectionBackground": "#00a8",
    "notebook.cellBorderColor": "#0001",
    "notebook.focusedCellBorder": "#007acc",
    "notebook.selectedCellBorder": "#007acc",
    "notebook.inactiveSelectedCellBorder": "#1a1a1a",
    "notebook.cellToolbarSeparator": "#7771",
    "editorBracketHighlight.foreground1": "#10b579",
    "editorBracketHighlight.foreground2": "#12bbd1",
    "editorBracketHighlight.foreground3": "#1b84fd",
    "editorBracketHighlight.foreground4": "#7148F1",
    "editorBracketHighlight.foreground5": "#CF0DE5",
    "editorBracketHighlight.foreground6": "#e50d84",
    "editorBracketMatch.background": "#00a",
    "editorBracketMatch.border": "#12bbd133",
    "editorBracketPairGuide.activeBackground1": "#10b579",
    "editorBracketPairGuide.activeBackground2": "#12bbd1",
    "editorBracketPairGuide.activeBackground3": "#1b84fd",
    "editorBracketPairGuide.activeBackground4": "#7148F1",
    "editorBracketPairGuide.activeBackground5": "#CF0DE5",
    "editorBracketPairGuide.activeBackground6": "#e50d84",
    "editorBracketPairGuide.background1": "#10b57988",
    "editorBracketPairGuide.background2": "#12bbd188",
    "editorBracketPairGuide.background3": "#1b84fd88",
    "editorBracketPairGuide.background4": "#7148F188",
    "editorBracketPairGuide.background5": "#CF0DE588",
    "editorBracketPairGuide.background6": "#e50d8488"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b9b"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#d6dbe5"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#f00"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#aa6666"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#ea9292"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#00d4eb"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.control.import",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#e69d00"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#ff6644"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#069b29"
      }
    },
    {
      "name": "HTML textContent and meta",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#5d8d33"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#f5800b"
      }
    },
    {
      "name": "XML tag fluff",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#ffaa11"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#008fe1"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#0f7dfa"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block", "meta.object.member"],
      "settings": {
        "foreground": "#77c7e1"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.object.property",
        "variable.other.property",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#9d49cd"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#ed5681"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "entity.name.type.enum",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#9258FF"
      }
    },
    {
      "name": "Unit",
      "scope": ["keyword.other.unit"],
      "settings": {
        "foreground": "#248ed3"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#e90e86"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.type.class",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#ee33b0"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name", "support.type.sys-types"],
      "settings": {
        "foreground": "#bf3ec6"
      }
    },

    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#e834d6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#008fe1"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#6688aa"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#888"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#074"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#074"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#6579d4"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e86"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#e86"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#074"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#6a3d88"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#ff446d"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ff6644"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#074"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6688aa"
      }
    },
